		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="age_category_type">
		<xs:annotation>
			<xs:documentation>
				Возрастная категория товара
				Детское - BABY
				Взрослое - ADULT
				Без возраста - NO_AGE
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="BABY"/>
			<xs:enumeration value="ADULT"/>
			<xs:enumeration value="NO_AGE"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="declaration_number_type">
		<xs:annotation>
			<xs:documentation>Регистрационный номер ДТ</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{8}\/[0-9]{6}\/[0-9]{7}"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="atk_type">
		<xs:annotation>
			<xs:documentation>Агрегированный таможенный код</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="25"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="vvod_fts_children_list_type">
		<xs:sequence>
			<xs:element name="product" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Список КИ/КИТУ в составе КИТУ/АТК для ввода в оборот импортных товаров с ФТС
